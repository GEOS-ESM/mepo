#!/usr/bin/env python3

import os
import sys
import traceback

# Version check
if sys.version_info < (3, 6, 0):
    sys.exit('ERROR: Python version needs to be >= 3.6.0')

# Add mepo.d to path
MEPO_D = os.path.join(os.path.dirname(os.path.realpath(__file__)), 'src', 'mepo')
sys.path.insert(0, MEPO_D)

if __name__ == '__main__':
    from main import main
    main()
